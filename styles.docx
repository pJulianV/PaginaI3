.metas {
    list-style-type: none;
}

/* Estilos para el reloj de arena */
.reloj {
    text-align: center;
}

.linea {
    width: 0;
    border: 1px solid black;
    margin: 2px 0;
    transition: width 0.5s ease;
}

/* Estilos para la lista de etiquetas HTML */
.etiquetas {
    list-style-type: decimal;
}